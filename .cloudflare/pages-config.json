{
  "production": {
    "build": {
      "command": "npm run build && npm run build:cloudflare",
      "cwd": "/",
      "watch_paths": ["src/**", "public/**", "next.config.ts", "package.json"]
    },
    "deployment": {
      "compatibility_date": "2025-10-03",
      "compatibility_flags": ["nodejs_compat"],
      "env_vars": {
        "NODE_VERSION": "22",
        "NEXT_TELEMETRY_DISABLED": "1",
        "NEXT_PUBLIC_GA_MEASUREMENT_ID": "G-6NW7LJG197"
      }
    }
  },
  "preview": {
    "build": {
      "command": "npm run build && npm run build:cloudflare",
      "cwd": "/"
    },
    "deployment": {
      "compatibility_date": "2025-10-03",
      "compatibility_flags": ["nodejs_compat"],
      "env_vars": {
        "NODE_VERSION": "22",
        "NEXT_TELEMETRY_DISABLED": "1",
        "NEXT_PUBLIC_GA_MEASUREMENT_ID": "G-6NW7LJG197"
      }
    }
  }
}
